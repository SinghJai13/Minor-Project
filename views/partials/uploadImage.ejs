<script class="jsbin" src="https://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
<link rel="stylesheet" href="/css/upload_Image.css">

<form action="/process-image" method="POST" enctype="multipart/form-data">
  <div class="file-upload">
    <button class="file-upload-btn" type="button" onclick="$('.file-upload-input').trigger( 'click' )">Add Image</button>
    <!-- ADDING DROPDOWNS -->
    <div class="drop_downs">

      <div class="drop_down_age">
        <select name="age" id="list" required>
          <option value="" disabled selected>Select Your Age Bracket</option>
          <option value="2">0-5</option>
          <option value="8">6-10</option>
          <option value="12">11-15</option>
          <option value="18">16-20</option>
          <option value="24">21-30</option>
          <option value="34">31-40</option>
          <option value="45">41-50</option>
          <option value="54">51-60</option>
          <option value="65">61-70</option>
          <option value="80">71-100</option>
        </select>
      </div>
      
      <div class="gender">
        <select name="gender" id ="gender" required>
          <option value="" disabled selected>Gender</option>
          <option value="0">Male</option>
          <option value="1">Female</option>
        </select>
      </div>

    </div>
    <!-- ENDING DROPDOWNS -->

    <div class="image-upload-wrap">
      <input name="myImage" class="file-upload-input" type='file' onchange="readURL(this);" accept="image/*" />
      <div class="drag-text">
        <h3>Drag and drop a file or select add Image</h3>
      </div>
    </div>
    <div class="file-upload-content">
      <img class="file-upload-image" src="#" alt="your image" />
      <div class="image-title-wrap">
        <button type="button" onclick="removeUpload()" class="remove-image">Remove <span class="image-title">Uploaded Image</span></button>
      </div>
      <div class="image-title-wrap">
        <button type="submit" class="remove-image" value="Processing">PROCESS IMAGE</button>
      </div>
    </div>
  </div>
</form>

<script src="/js/upload_Image.js"></script>